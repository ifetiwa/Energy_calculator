{
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public",
  "functions": {
    "api/calculations.ts": {
      "runtime": "@vercel/node@2.15.10"
    }
  },
  "rewrites": [
    {
      "source": "/api/calculations/:path*",
      "destination": "/api/calculations"
    },
    {
      "source": "/api/calculations",
      "destination": "/api/calculations"
    }
  ]
}
